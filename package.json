    "eslint-config-prettier": "^8.6.0",
    "import-sort-style-wes": "^3.0.2",
    "prettier": "2.8.4",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-node": "^10.9.1",
    "typescript": "5.0.2"
